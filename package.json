{
  "name": "token-hider",
  "version": "1.0.0",
  "description": "a token hider using netlify functions",
  "main": "app.js",
  "scripts": {
    "start": "npm run lambda:dev",
    "build": "npm run site:build",
    "site:dev": "parcel serve src/site/index.html --public-url",
    "site:build": "parcel build src/site/index.html --public-url",
    "lambda:dev": "netlify dev -c 'npm run site:dev'"
  },
  "author": "Dennis Padiernos",
  "license": "ISC",
  "devDependencies": {
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.19.1",
    "netlify-cli": "^2.12.0"
  },
  "dependencies": {
    "parcel-bundler": "^1.12.3",
    "@google/maps": "^0.5.5",
    "geoip-lite": "^1.3.7",
    "npm-run-all": "^4.1.5",
    "qs": "^6.8.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "reset.css": "^2.0.2"
  }
}
